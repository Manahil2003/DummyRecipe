//import 'dart:io';
import 'dummy_recipe1.dart';
import 'dummy_recipes.dart';

class SelectiveRecipe {
  final String id;
  final String title;
  final int duration;

  SelectiveRecipe.init(
      {required this.id, required this.title, required this.duration});

  @override
  String toString() {
    return '$id \n$title \n$duration';
  }
}
